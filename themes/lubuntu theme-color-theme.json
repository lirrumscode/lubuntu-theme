	  "list.focusForeground": "#f5faff",
	  "list.inactiveSelectionBackground": "#151d29",
	  "list.inactiveSelectionForeground": "#f5faff",
	  "list.highlightForeground": "#6FC1FF",
	  "editor.background": "#151d29",
	  "editor.foreground": "#f5faff",
	  "sideBar.background": "#1e293b",
	  "sideBar.foreground": "#bfdbfe",
	  "sideBar.border": "#1e293b",
	  "terminalCursor.background": "#f5faff",
	  "terminalCursor.foreground": "#bfdbfe",
	  "terminal.ansiBlue": "#bfdbfe",
	  "terminal.ansiBrightBlack": "#94a3b8",
	  "terminal.ansiBrightBlue": "#60a5fa",
	  "activityBarBadge.background": "#60a5fa",
	  "sideBarTitle.foreground": "#bfdbfe",
	  "statusBar.background": "#1e293b",
	  "statusBar.foreground": "#f5faff",
	  "tab.activeBackground": "#334155",
	  "tab.activeForeground": "#f5faff",
	  "tab.activeBorder": "#93c5fd",
	  "tab.inactiveBackground": "#64748b",
	  "tab.inactiveForeground": "#f5faff",
	  "badge.background": "#93c5fd",
	  "badge.foreground": "#1e293b",
	  "titleBar.activeBackground": "#1e293b",
	  "titleBar.activeForeground": "#f5faff",
	  "titleBar.inactiveBackground": "#1e293b",
	  "titleBar.inactiveForeground": "#f5faff",
	  "activityBar.background": "#151d29",
	  "scrollbarSlider.background": "#93c5fd",
	  "editorCodeLens.foreground": "#93c4fd35",
	  "editorWhitespace.foreground": "#1e293b",
	  "editor.selectionBackground": "#93c4fd35"
	},
	"tokenColors": [
	  {
		"name": "Comment",
		"scope": ["comment", "punctuation.definition.comment"],
		"settings": {
		  "fontStyle": "italic",
		  "foreground": "#646464"
		}
	  },
	  {
		"name": "Variables",
		"scope": ["variable", "string constant.other.placeholder"],
		"settings": {
		  "foreground": "#60a5fa"
		}
	  },
	  {
		"name": "Colors",
		"scope": ["constant.other.color"],
		"settings": {
		  "foreground": "#ffffff"
		}
	  },
	  {
		"name": "Invalid",
		"scope": ["invalid", "invalid.illegal"],
		"settings": {
		  "foreground": "#1e293b"
		}
	  },
	  {
		"name": "Keyword, Storage",
		"scope": ["keyword", "storage.type", "storage.modifier"],
		"settings": {
		  "foreground": "#93c5fd"
		}
	  },
	  {
		"name": "Operator, Misc",
		"scope": [
		  "keyword.control",
		  "constant.other.color",
		  "punctuation",
		  "meta.tag",
		  "punctuation.definition.tag",
		  "punctuation.separator.inheritance.php",
		  "punctuation.definition.tag.html",
		  "punctuation.definition.tag.begin.html",
		  "punctuation.definition.tag.end.html",
		  "punctuation.section.embedded",
		  "keyword.other.template",
		  "keyword.other.substitution"
		],
		"settings": {
		  "foreground": "#64748b"
		}
	  },
	  {
		"name": "Tag",
		"scope": [
		  "entity.name.tag",
		  "meta.tag.sgml",
		  "markup.deleted.git_gutter"
		],
		"settings": {
		  "foreground": "#64748b"
		}
	  },
	  {
		"name": "Function, Special Method",
		"scope": [
		  "entity.name.function",
		  "meta.function-call",
		  "variable.function",
		  "support.function",
		  "keyword.other.special-method"
		],
		"settings": {
		  "foreground": "#e2e8f0"
		}
	  },
	  {
		"name": "Block Level Variables",
		"scope": ["meta.block variable.other"],
		"settings": {
		  "foreground": "#3b82f6"
		}
	  },
	  {
		"name": "Other Variable, String Link",
		"scope": ["support.other.variable", "string.other.link"],
		"settings": {
		  "foreground": "#1d4ed8"
		}
	  },
	  {
		"name": "Number, Constant, Function Argument, Tag Attribute, Embedded",
		"scope": [
		  "constant.numeric",
		  "constant.language",
		  "support.constant",
		  "constant.character",
		  "constant.escape",
		  "variable.parameter",
		  "keyword.other.unit",
		  "keyword.other"
		],
		"settings": {
		  "foreground": "#e2e8f0"
		}
	  },
	  {
		"name": "String, Symbols, Inherited Class, Markup Heading",
		"scope": [
		  "string",
		  "constant.other.symbol",
		  "constant.other.key",
		  "entity.other.inherited-class",
		  "markup.heading",
		  "markup.inserted.git_gutter",
		  "meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js"
		],
		"settings": {
		  "foreground": "#60a5fa"
		}
	  },
	  {
		"name": "Class, Support",
		"scope": [
		  "entity.name",
		  "support.type",
		  "support.class",
		  "support.other.namespace.use.php",
		  "meta.use.php",
		  "support.other.namespace.php",
		  "markup.changed.git_gutter",
		  "support.type.sys-types"
		],
		"settings": {
		  "foreground": "#60a5fa"
		}
	  },
	  {
		"name": "Entity Types",
		"scope": ["support.type"],
		"settings": {
		  "foreground": "#cbd5e1"
		}
	  },
	  {
		"name": "CSS Class and Support",
		"scope": [
		  "source.css support.type.property-name",
		  "source.sass support.type.property-name",
		  "source.scss support.type.property-name",
		  "source.less support.type.property-name",
		  "source.stylus support.type.property-name",
		  "source.postcss support.type.property-name"
		],
		"settings": {
		  "foreground": "#f5faff"
		}
	  },
	  {
		"name": "Sub-methods",
		"scope": [
		  "entity.name.module.js",
		  "variable.import.parameter.js",
		  "variable.other.class.js"
		],
		"settings": {
		  "foreground": "#1e3a8a"
		}
	  },
	  {
		"name": "Language methods",
		"scope": ["variable.language"],
		"settings": {
		  "fontStyle": "italic",
		  "foreground": "#516797"
		}
	  },
	  {
		"name": "entity.name.method.js",
		"scope": ["entity.name.method.js"],
		"settings": {
		  "fontStyle": "italic",
		  "foreground": "#516797"
		}
	  },
	  {
		"name": "meta.method.js",
		"scope": [
		  "meta.class-method.js entity.name.function.js",
		  "variable.function.constructor"
		],
		"settings": {
		  "foreground": "#516797"
		}
	  },
	  {
		"name": "Attributes",
		"scope": ["entity.other.attribute-name"],
		"settings": {
		  "foreground": "#2563eb"
		}
	  },
	  {
		"name": "HTML Attributes",
		"scope": [
		  "text.html.basic entity.other.attribute-name.html",
		  "text.html.basic entity.other.attribute-name"
		],
		"settings": {
		  "fontStyle": "italic",
		  "foreground": "#93c5fd"
		}
	  },
	  {
		"name": "CSS Classes",
		"scope": ["entity.other.attribute-name.class"],
		"settings": {
		  "foreground": "#93c5fd"
		}
	  },
	  {
		"name": "CSS ID's",
		"scope": ["source.sass keyword.control"],
		"settings": {
		  "foreground": "#516797"
		}
	  },
	  {
		"name": "Inserted",
		"scope": ["markup.inserted"],
		"settings": {
		  "foreground": "#f1f5f9"
		}
	  },
	  {
		"name": "Deleted",
		"scope": ["markup.deleted"],
		"settings": {
		  "foreground": "#334155"
		}
	  },
	  {
		"name": "Changed",
		"scope": ["markup.changed"],
		"settings": {
		  "foreground": "#fcfcfd"
		}
	  },
	  {
		"name": "Regular Expressions",
		"scope": ["string.regexp"],
		"settings": {
		  "foreground": "#cbd5e1"
		}
	  },
	  {
		"name": "Escape Characters",
		"scope": ["constant.character.escape"],
		"settings": {
		  "foreground": "#cbd5e1"
		}
	  },
	  {
		"name": "URL",
		"scope": ["*url*", "*link*", "*uri*"],
		"settings": {
		  "fontStyle": "underline"
		}
	  },
	  {
		"name": "Decorators",
